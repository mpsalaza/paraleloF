resultado = 5 + 3 * 2 ** 2 - 8 / 4
print(resultado)


resultado = (5 + 3) * (2 ** 2 - 8) / 4
print(resultado)

# Definir número
num = 16

# Exponente (cuadrado)
cuadrado = num ** 2
print(f"El cuadrado de {num} es: {cuadrado}")

# Raíz cuadrada
raiz = num ** 0.5
print(f"La raíz cuadrada de {num} es: {raiz}")

# Módulo (resto de la división)
resto = num % 5
print(f"El resto de {num} dividido entre 5 es: {resto}")